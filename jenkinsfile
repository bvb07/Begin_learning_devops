pipeline {
    agent any
    stages {
        stage('start'){
            steps{
                sh '''
                cd compose
                docker-compose up -d --build
                '''
            }
        }
        stage('Check') {
            steps {
                sh 'ls -a'
            }
        }
        stage('Stop') {
            steps {
                sh 'docker-compose down --volumes'
            }
        }

        
    }
}