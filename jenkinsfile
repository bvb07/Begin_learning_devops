pipeline {
    agent any
    stages {
        stage('start'){
            steps{
                sh '''
                cd compose
                pwd
                docker --version
                ls -a
                '''
                echo '*************************************************'
            }
          }
        stage('build'){
            steps{
                sh '''
                cd compose
                docker-compose up -d
                pwd
                '''
                echo '*************************************************'
              }
          }
        stage('stop end'){
            steps{
                sh '''
                cd compose
                docker-compose down
                pwd
                '''
                echo '*************************************************'
           }
        }
       


        
    }
}
